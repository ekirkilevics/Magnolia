<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "module.dtd" >
<module>
  <name>cache</name>
  <class>info.magnolia.cms.cache.Engine</class>
  <version>3.0</version>
  <displayName>Magnolia pluggable Cache</displayName>
  <servlets>
    <servlet>
      <name>CacheServlet</name>
      <class>info.magnolia.cms.cache.CacheGeneratorServlet</class>
      <comment>Cache Generator Servlet</comment>
      <mappings>
        <mapping>/.magnolia/cache-all/*</mapping>
      </mappings>
    </servlet>
  </servlets>
</module>
