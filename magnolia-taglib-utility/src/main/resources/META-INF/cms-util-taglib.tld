    <attribute>
      <description>html pass through attribute.</description>
      <name>id</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <description>html pass through attribute.</description>
      <name>width</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <description>html pass through attribute.</description>
      <name>height</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <description>
        The content object to use.
      </description>
      <name>contentNode</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <description>
        The uuid to use for finding the content.
        You must define the repository attribute if the content is not stored in the website repository.
      </description>
      <name>uuid</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <description>
        The absolute path to the content.
        You must define the repository attribute if the content is not stored in the website repository.
      </description>
      <name>path</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
