<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "module.dtd" >
<module>
  <name>mail</name>
  <displayName>Magnolia Mail Module</displayName>
  <class>info.magnolia.module.mail.MailModule</class>
  <versionHandler>info.magnolia.module.mail.setup.MailModuleVersionHandler</versionHandler>
  <version>${project.version}</version>
  <components>
    <id>main</id>
    <type-mapping>
      <type>info.magnolia.module.mail.handlers.MgnlMailHandler</type>
      <implementation>info.magnolia.module.mail.handlers.SimpleMailHandler</implementation>
    </type-mapping>
  </components>
  <dependencies>
    <dependency>
      <name>adminInterface</name>
      <version>4.5.2/*</version>
    </dependency>
  </dependencies>
</module>
