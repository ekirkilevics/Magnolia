<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE Repositories [
    <!ELEMENT Repositories (Repository+)>
    <!ELEMENT Repository EMPTY>
    <!ATTLIST Repository
        name CDATA #REQUIRED
        id CDATA #REQUIRED
        loadOnStartup CDATA #REQUIRED>]>

<!--

 @version 2.0
-->



<Repositories>

        <!-- magnolia default website repository -->
        <Repository name="website" id="website" loadOnStartup="true" />
        <!-- magnolia default config repository -->
        <Repository name="config" id="config" loadOnStartup="true" />
        <!-- magnolia user repository -->
        <Repository name="users" id="users" loadOnStartup="true" />
        <!-- magnolia default user roles repository -->
        <Repository name="userroles" id="userroles" loadOnStartup="true" />

</Repositories>